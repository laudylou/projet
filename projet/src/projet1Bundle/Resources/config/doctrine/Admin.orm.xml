<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                  http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">


    <entity name="projet1Bundle\Entity\Admin" table="admin" repository-class="projet1Bundle\repository\Admin">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>
        <field name="nom" type="string" column="titre" length="100" />
        <field name="prenom" type="string" column="img" length="100" />
        <field name="identifiant" type="string" column="contenu" length="100" />
        <field name="motDePasse" type="string" column="Mot_de_passe" length="100" />

    </entity>
</doctrine-mapping>