<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                  http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">


    <entity name="projet1Bundle\Entity\Commentaire" table="commentaire" repository-class="projet1Bundle\repository\Commentaire">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>
        <field name="pseudo" type="string" column="pseudo" length="100" />
        <field name="texte" type="string" column="texte" length="100" />
        <field name="date" type="datetime" column="date" length="100" />
    </entity>
</doctrine-mapping>